fun Cat(name) {

    let foodEaten = 0;

    fun getName() { return name; }
    fun meow() { return "Cat " + name + ": meowww!"; }
    fun isHungry() { return foodEaten < 5; }
    fun eat(food) { foodEaten += food; }

    return obj {
        "hungry": isHungry,
        "name": getName,
        "meow": meow,
        "eat": eat,
    };
}

let cat = Cat("Tosha");
echo cat.meow();
echo cat.hungry();
for (i from 1 to 10) cat.eat(1);
echo cat.hungry();